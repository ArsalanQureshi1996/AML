    <script>


        function openNav(id) {
         document.getElementById("myNav").style.height = "100%";
         console.log(id);
         document.getElementById("check4").value = id;
        }

        function closeNav() {
        document.getElementById("myNav").style.height = "0%";
        }

       
       
        


    </script>
   
   </body>
</html>
<!-- end document-->
